{
  "expo": {
    "name": "social-media-ad",
    "slug": "social-media-ad",
    "scheme": "asmt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash/asm.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash/asm.png",
      "resizeMode": "contain",
      "backgroundColor": "#0073E6"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash/asm.png",
        "backgroundColor": "#0073E6"
      },
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ],
      "package": "com.capteze.socialmediaad"
    },
    "web": {
      "favicon": "./assets/images/splash/asm.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/plus-jakatar-sans/PlusJakartaSans.ttf"
          ]
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow ASM to use Face ID."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "697755c5-0299-4d35-a6f8-0c7fe38895ae"
      }
    }
  }
}
